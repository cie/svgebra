{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "preserve",
    "jsxImportSource": "mobx-jsx",
    "lib": ["DOM", "es2016"],
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true /* Skip type checking all .d.ts files. */
  }
}
